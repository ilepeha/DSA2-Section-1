<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <!-- Debug-specific user settings -->
    <StartAction>Program</StartAction>
    <StartProgram>bin\Debug\Section1.exe</StartProgram>
    <StartWorkingDirectory>$(ProjectDir)</StartWorkingDirectory>
    <StartArguments></StartArguments>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <!-- Release-specific user settings -->
    <StartAction>None</StartAction>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>

  <PropertyGroup>
    <!-- Global user settings -->
    <LastActiveSolutionConfig>Debug|AnyCPU</LastActiveSolutionConfig>
    <LastActiveSolutionPlatform>AnyCPU</LastActiveSolutionPlatform>
    <AlwaysStartWhenDebugging>true</AlwaysStartWhenDebugging>
    <SaveSettingsOnExit>true</SaveSettingsOnExit>
    <ShowOutputWindowWhenBuildStarts>true</ShowOutputWindowWhenBuildStarts>
  </PropertyGroup>
</Project>
